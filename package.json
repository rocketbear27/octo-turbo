{
  "name": "octo-turbo",
  "version": "1.0.9",
  "description": "An app created by RocketBear27, helped and supported by Cory House's Pluralsight Course",
  "scripts": {
    "app": "npm-run-all --parallel svr check lint:watch test:watch",
    "svr": "babel-node scripts/svr.js",
    "check": "nsp check",
    "lint": "esw webpack.config.* src scripts --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress scripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node scripts/build.js",
    "postbuild": "babel-node scripts/distSvr.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rocketbear27/octo-turbo.git"
  },
  "author": "RocketBear27",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rocketbear27/octo-turbo/issues"
  },
  "homepage": "https://github.com/rocketbear27/octo-turbo#readme",
  "dependencies": {
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "1.6.2",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "eslint": "3.8.1",
    "eslint-plugin-import": "2.0.1",
    "eslint-watch": "2.1.14",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.22.0",
    "jsdom": "9.8.0",
    "json-schema-faker": "0.3.6",
    "json-server": "0.8.22",
    "localtunnel": "1.8.1",
    "mocha": "3.1.2",
    "nock": "8.1.0",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "numeral": "1.5.3",
    "open": "0.0.5",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5"
  }
}
